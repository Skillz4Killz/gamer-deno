{
    "private": true,
    "name": "gamerbot-website",
    "description": "",
    "license": "",
    "scripts": {
        "build": "npm run build:css && cross-env NODE_ENV=production remix build",
        "build:css": "tailwindcss -m -i ./app/styles/app.css -o ./app/app.css",
        "de": "concurrently 'npm run dev:css' 'cross-env NODE_ENV=development remix dev'",
        "dev": "remix setup node && concurrently 'npm run dev:css' 'cross-env NODE_ENV=development remix build && run-p dev:*'",
        "dev:css": "tailwindcss -w -i ./app/styles/app.css -o ./app/app.css",
        "dev:node": "cross-env NODE_ENV=development nodemon -r dotenv/config ./build/index.js",
        "dev:remix": "cross-env REMIX_ENV=development remix watch",
        "postinstall": "remix setup node",
        "start": "cross-env NODE_ENV=production node -r dotenv/config remix-serve build",
        "dbpull": "prisma db pull"
    },
    "dependencies": {
        "@prisma/client": "^4.5.0",
        "@remix-run/express": "^1.7.4",
        "@remix-run/node": "^1.7.4",
        "@remix-run/react": "^1.7.4",
        "@remix-run/serve": "^1.7.4",
        "@types/accept-language-parser": "^1.5.3",
        "accept-language-parser": "^1.5.0",
        "compression": "^1.7.4",
        "cross-env": "^7.0.3",
        "discord-oauth2": "^2.10.1",
        "dotenv": "^16.0.3",
        "fernet": "^0.4.0",
        "generic-pool": "^3.9.0",
        "i18next": "^21.10.0",
        "i18next-browser-languagedetector": "^7.0.0",
        "i18next-fs-backend": "^2.0.0",
        "i18next-http-backend": "^2.0.0",
        "ioredis": "^5.2.3",
        "morgan": "^1.10.0",
        "npm-run-all": "^4.1.5",
        "react": "^18.2.0",
        "react-countdown": "^2.3.3",
        "react-countup": "^6.3.2",
        "react-dom": "^18.2.0",
        "react-i18next": "^11.18.6",
        "react-icons": "^4.6.0",
        "react-router-dom": "^6.4.2",
        "react-simply-carousel": "^8.4.0",
        "react-slick": "^0.29.0",
        "remix": "^1.7.4",
        "remix-i18next": "^4.1.1",
        "remix-image": "^1.3.3",
        "remix-utils": "^3.4.0",
        "theme-change": "^2.2.0"
    },
    "engines": {
        "node": ">=14"
    },
    "sideEffects": false,
    "devDependencies": {
        "@remix-run/dev": "^1.7.4",
        "@types/i18next-fs-backend": "^1.1.2",
        "@types/react": "^18.0.24",
        "@types/react-dom": "^18.0.8",
        "@types/react-slick": "^0.23.10",
        "autoprefixer": "^10.4.13",
        "concurrently": "^7.5.0",
        "daisyui": "^2.36.1",
        "nodemon": "^2.0.20",
        "postcss": "^8.4.18",
        "prisma": "^4.5.0",
        "tailwindcss": "^3.2.1",
        "typescript": "^4.8.4"
    }
}
